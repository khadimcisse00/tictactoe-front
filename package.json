{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "mjml": "mjml emails/verification.mjml -o emails/verification.html && mjml emails/reset.mjml -o emails/reset.html",
    "socket": "node BackEnd/server.js",
    "backend:install": "cd BackEnd && npm install",
    "dev-all": "concurrently \"npm run dev\" \"npm run socket\""
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cookie": "^0.6.0",
    "daisyui": "^4.12.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.556.0",
    "mjml": "^4.15.3",
    "mysql2": "^3.11.0",
    "next": "14.2.15",
    "nodemailer": "^6.9.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.4.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mjml": "^4.7.4",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.13",
    "typescript": "5.6.3"
  }
